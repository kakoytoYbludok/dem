<Window x:Class="ServiceTechno.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ServiceTechno"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbLogin" HorizontalAlignment="Center" Margin="0,159,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="218" Background="Red" Height="33"/>
        <TextBox x:Name="tbPass" HorizontalAlignment="Center" Margin="0,197,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="218" Background="Red" Height="38"/>
        <Button x:Name="Button" Content="Button" HorizontalAlignment="Center" Margin="0,271,0,0" VerticalAlignment="Top" Click="Button_Click" Height="58" Width="241"/>

    </Grid>
</Window>
